{% extends 'baselayout.html.twig' %}

{% block title %}Region{% endblock %}

{% block main %}
    <h1>Region</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ region.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ region.name }}</td>
            </tr>
            <tr>
                <th>Presentation</th>
                <td>{{ region.presentation }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ region.country }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('client_region_index') }}">back to list</a>



{% endblock %}

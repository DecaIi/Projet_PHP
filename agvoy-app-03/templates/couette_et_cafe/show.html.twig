{% extends 'base.html.twig' %}

{% block title %}Region{% endblock %}

{% block main %}
    <h1>Region</h1>

    <table class="table">
        <tbody>
        {% foreach room in rooms %}
            <tr>
                <td><h1>{{ room.name }}</h1></td>
                <td>{{  room.description }}</td>
            </tr>
        </tbody>
    </table>

{% endblock %}